

check process logio-server
	with matching "node /usr/bin/log.io-server"
	  start program = "/bin/sh /var/logio.sh start"
	  stop program = "/bin/sh /var/logio.sh stop"
	  
	  
check process logio-harvester
	with matching "node /usr/bin/log.io-harvester"
	  start program = "/bin/sh /var/logio.sh start"
	  stop program = "/bin/sh /var/logio.sh stop"
  
  